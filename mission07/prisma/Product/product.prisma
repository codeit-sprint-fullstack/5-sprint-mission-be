model Product {
  id             String           @id @default(uuid())
  name           String
  description    String
  price          Int
  createdAt      DateTime         @default(now())
  updatedAt      DateTime         @updatedAt
  CommentProduct CommentProduct[]
  LikesProduct   LikesProduct[]
  userId         String
  User           User             @relation(fields: [userId], references: [id])
}
